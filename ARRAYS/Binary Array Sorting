

#include <bits/stdc++.h> 
using namespace std;

void SortBinaryArray(int binArray[], int n)
{   int count;
    for(int i=0;i<n;i++){
        if(binArray[i]==0){
            count=count+1;
        }
    }
    for(int i=0;i<count;i++){
        cout<<"0 ";
    }
    for(int i=count;i<n;i++){
        cout<<"1 ";
    }
}


int main() {	    
	    int n;
	    cin>>n;
	    int binArray[n];
	    for(int i = 0; i  < n; i++)
	      cin>>binArray[i];
	    SortBinaryArray(binArray, n);  
	    cout<<endl;
	
	return 0;
}
