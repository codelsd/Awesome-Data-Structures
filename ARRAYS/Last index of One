#include <iostream>
using namespace std;

int main() {
    int t;
    cin>>t;
    while(t--){
        string str;
        cin>>str;
        int l=str.length();
        int index=-1;
        for(int i=0;i<l;i++){
            if(str[i]=='1'){
                index=i;
            }
        }
        cout<<index<<endl;
    }
	return 0;
}


